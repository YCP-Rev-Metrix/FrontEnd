<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp2.FilePage"
             Title="Sim Page">
    <VerticalStackLayout
        VerticalOptions="Center">
        
        <Label
            Text="I am going to attempt to implement the FilePicker and fileCreation functionality on this page"
            SemanticProperties.HeadingLevel="Level1"
            FontSize="32"
            HorizontalOptions="Center" />
        
        
        
        <Button 
            x:Name="HomeBtn"
            Text="Home"
            SemanticProperties.Hint="Navigate to main page when you click"
            Clicked="OnHomeBtnClicked"
            HorizontalOptions="Center" />

        
    </VerticalStackLayout>
</ContentPage>